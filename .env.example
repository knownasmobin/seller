BOT_TOKEN=your_telegram_bot_token_here
ADMIN_ID=123456789
ADMIN_CARD_NUMBER=1234-5678-9012-3456

# Oxapay Crypto Gateway
OXAPAY_MERCHANT_KEY=your_oxapay_merchant_key_here

# Server Initialization (These will be automatically added to the Database on first run)
MARZBAN_URL=https://your-marzban-domain.com
MARZBAN_USERNAME=admin
MARZBAN_PASSWORD=secret

WGPORTAL_URL=https://your-wgportal-domain.com
WGPORTAL_USERNAME=admin
WGPORTAL_PASSWORD=secret

# Bot Branding (used as header in WireGuard configs)
BOT_NAME=ghostwire t.me/theghostwirebot

# WireSock Split Tunneling - comma-separated list of .exe files
# These are appended as #@ws:AllowedApps in every WireGuard config
# Leave empty to disable WireSock extensions
WIRESOCK_ALLOWED_APPS=7DaysToDie.exe, cs2.exe, dota2.exe, VALORANT.exe, Fortnite.exe
